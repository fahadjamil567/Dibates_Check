{
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python@2.0.1",
            "config": {
                "runtime": "python3.7",
                "maxLambdaSize": "10mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
} 